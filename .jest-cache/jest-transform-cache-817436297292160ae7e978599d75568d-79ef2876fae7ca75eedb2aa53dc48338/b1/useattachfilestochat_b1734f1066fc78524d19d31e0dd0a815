      cov_1xizh0u1dj().b[2][1]++;
    }
    let filesToAdd =
    /* istanbul ignore next */
    (cov_1xizh0u1dj().s[13]++, []);
    /* istanbul ignore next */
    cov_1xizh0u1dj().s[14]++;
    setChatFiles(prev => {
      /* istanbul ignore next */
      cov_1xizh0u1dj().f[3]++;
      const existingIds =
      /* istanbul ignore next */
      (cov_1xizh0u1dj().s[15]++, new Set(prev.map(file => {
        /* istanbul ignore next */
        cov_1xizh0u1dj().f[4]++;
        cov_1xizh0u1dj().s[16]++;
        return file.id;
      })));
      const uniqueRecords =
      /* istanbul ignore next */
      (cov_1xizh0u1dj().s[17]++, filesToAttach.filter(file => {
        /* istanbul ignore next */
        cov_1xizh0u1dj().f[5]++;
        cov_1xizh0u1dj().s[18]++;
        return /* istanbul ignore next */(cov_1xizh0u1dj().b[4][0]++, file) &&
        /* istanbul ignore next */
        (cov_1xizh0u1dj().b[4][1]++, !existingIds.has(file.id));
      }));
      /* istanbul ignore next */
      cov_1xizh0u1dj().s[19]++;
      filesToAdd = uniqueRecords.map(mapToChatFile);
      /* istanbul ignore next */
      cov_1xizh0u1dj().s[20]++;
      if (filesToAdd.length === 0) {
        /* istanbul ignore next */
        cov_1xizh0u1dj().b[5][0]++;
        cov_1xizh0u1dj().s[21]++;
        return prev;
      } else
      /* istanbul ignore next */
      {
        cov_1xizh0u1dj().b[5][1]++;
      }
      cov_1xizh0u1dj().s[22]++;
      return [...prev, ...filesToAdd];
    });
    /* istanbul ignore next */
    cov_1xizh0u1dj().s[23]++;
    if (filesToAdd.length === 0) {
      /* istanbul ignore next */
      cov_1xizh0u1dj().b[6][0]++;
      cov_1xizh0u1dj().s[24]++;
      sonner_1.toast.info("Los archivos seleccionados ya están disponibles en el chat.");
      /* istanbul ignore next */
      cov_1xizh0u1dj().s[25]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1xizh0u1dj().b[6][1]++;
    }
    cov_1xizh0u1dj().s[26]++;
    setShowFilesDisplay(true);
    /* istanbul ignore next */
    cov_1xizh0u1dj().s[27]++;
    setUseRetrieval(true);
    const label =
    /* istanbul ignore next */
    (cov_1xizh0u1dj().s[28]++,
    /* istanbul ignore next */
    (cov_1xizh0u1dj().b[7][0]++, options.sourceName) ||
    /* istanbul ignore next */
    (cov_1xizh0u1dj().b[7][1]++, filesToAdd.length === 1 ?
    /* istanbul ignore next */
    (cov_1xizh0u1dj().b[8][0]++, filesToAdd[0].name) :
    /* istanbul ignore next */
    (cov_1xizh0u1dj().b[8][1]++, `${filesToAdd.length} archivos`)));
    /* istanbul ignore next */
    cov_1xizh0u1dj().s[29]++;
    sonner_1.toast.success(`Se añadieron ${filesToAdd.length} archivo${filesToAdd.length > 1 ?
    /* istanbul ignore next */
    (cov_1xizh0u1dj().b[9][0]++, "s") :
    /* istanbul ignore next */
    (cov_1xizh0u1dj().b[9][1]++, "")} de ${label} al contexto del chat.`);
  }, [setChatFiles, setShowFilesDisplay, setUseRetrieval]);
};
/* istanbul ignore next */
cov_1xizh0u1dj().s[30]++;
exports.useAttachFilesToChat = useAttachFilesToChat;
