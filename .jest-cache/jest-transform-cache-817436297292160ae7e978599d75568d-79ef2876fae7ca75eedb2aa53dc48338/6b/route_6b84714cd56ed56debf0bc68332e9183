        }));
        /* istanbul ignore next */
        cov_1achjk6u4t().s[120]++;
        if (!schemaDetail) {
          /* istanbul ignore next */
          cov_1achjk6u4t().b[39][0]++;
          cov_1achjk6u4t().s[121]++;
          throw new Error(`Function ${functionName} not found in any schema`);
        } else
        /* istanbul ignore next */
        {
          cov_1achjk6u4t().b[39][1]++;
        }
        const pathTemplate =
        /* istanbul ignore next */
        (cov_1achjk6u4t().s[122]++, Object.keys(schemaDetail.routeMap).find(key => {
          /* istanbul ignore next */
          cov_1achjk6u4t().f[11]++;
          cov_1achjk6u4t().s[123]++;
          return schemaDetail.routeMap[key] === functionName;
        }));
        /* istanbul ignore next */
        cov_1achjk6u4t().s[124]++;
        if (!pathTemplate) {
          /* istanbul ignore next */
          cov_1achjk6u4t().b[40][0]++;
          cov_1achjk6u4t().s[125]++;
          throw new Error(`Path for function ${functionName} not found`);
        } else
        /* istanbul ignore next */
        {
          cov_1achjk6u4t().b[40][1]++;
        }
        const path =
        /* istanbul ignore next */
        (cov_1achjk6u4t().s[126]++, pathTemplate.replace(/:(\w+)/g, (_, paramName) => {
          /* istanbul ignore next */
          cov_1achjk6u4t().f[12]++;
          const value =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[127]++, parsedArgs.parameters[paramName]);
          /* istanbul ignore next */
          cov_1achjk6u4t().s[128]++;
          if (!value) {
            /* istanbul ignore next */
            cov_1achjk6u4t().b[41][0]++;
            cov_1achjk6u4t().s[129]++;
            throw new Error(`Parameter ${paramName} not found for function ${functionName}`);
          } else
          /* istanbul ignore next */
          {
            cov_1achjk6u4t().b[41][1]++;
          }
          cov_1achjk6u4t().s[130]++;
          return encodeURIComponent(value);
        }));
        /* istanbul ignore next */
        cov_1achjk6u4t().s[131]++;
        if (!path) {
          /* istanbul ignore next */
          cov_1achjk6u4t().b[42][0]++;
          cov_1achjk6u4t().s[132]++;
          throw new Error(`Path for function ${functionName} not found`);
        } else
        /* istanbul ignore next */
        {
          cov_1achjk6u4t().b[42][1]++;
        }
        // Determine if the request should be in the body or as a query
        const isRequestInBody =
        /* istanbul ignore next */
        (cov_1achjk6u4t().s[133]++, schemaDetail.requestInBody);
        let data =
        /* istanbul ignore next */
        (cov_1achjk6u4t().s[134]++, {});
        /* istanbul ignore next */
        cov_1achjk6u4t().s[135]++;
        if (isRequestInBody) {
          /* istanbul ignore next */
          cov_1achjk6u4t().b[43][0]++;
          // If the type is set to body
          let headers =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[136]++, {
            "Content-Type": "application/json"
          });
          // Check if custom headers are set
          const customHeaders =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[137]++, schemaDetail.headers); // Moved this line up to the loop
          // Check if custom headers are set and are of type string
          /* istanbul ignore next */
          cov_1achjk6u4t().s[138]++;
          if (
          /* istanbul ignore next */
          (cov_1achjk6u4t().b[45][0]++, customHeaders) &&
          /* istanbul ignore next */
          (cov_1achjk6u4t().b[45][1]++, typeof customHeaders === "string")) {
            /* istanbul ignore next */
            cov_1achjk6u4t().b[44][0]++;
            let parsedCustomHeaders =
            /* istanbul ignore next */
            (cov_1achjk6u4t().s[139]++, JSON.parse(customHeaders));
            /* istanbul ignore next */
            cov_1achjk6u4t().s[140]++;
            headers = {
              ...headers,
              ...parsedCustomHeaders
            };
          } else
          /* istanbul ignore next */
          {
            cov_1achjk6u4t().b[44][1]++;
          }
          const fullUrl =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[141]++, schemaDetail.url + path);
          const bodyContent =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[142]++,
          /* istanbul ignore next */
          (cov_1achjk6u4t().b[46][0]++, parsedArgs.requestBody) ||
          /* istanbul ignore next */
          (cov_1achjk6u4t().b[46][1]++, parsedArgs));
          const requestInit =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[143]++, {
            method: "POST",
            headers,
            body: JSON.stringify(bodyContent) // Use the extracted requestBody or the entire parsedArgs
          });
          const response =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[144]++, await fetch(fullUrl, requestInit));
          /* istanbul ignore next */
          cov_1achjk6u4t().s[145]++;
          if (!response.ok) {
            /* istanbul ignore next */
            cov_1achjk6u4t().b[47][0]++;
            cov_1achjk6u4t().s[146]++;
            data = {
              error: response.statusText
            };
          } else {
            /* istanbul ignore next */
            cov_1achjk6u4t().b[47][1]++;
            cov_1achjk6u4t().s[147]++;
            data = await response.json();
          }
        } else {
          /* istanbul ignore next */
          cov_1achjk6u4t().b[43][1]++;
          // If the type is set to query
          const queryParams =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[148]++, new URLSearchParams(parsedArgs.parameters).toString());
          const fullUrl =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[149]++, schemaDetail.url + path + (queryParams ?
          /* istanbul ignore next */
          (cov_1achjk6u4t().b[48][0]++, "?" + queryParams) :
          /* istanbul ignore next */
          (cov_1achjk6u4t().b[48][1]++, "")));
          let headers =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[150]++, {});
          // Check if custom headers are set
          const customHeaders =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[151]++, schemaDetail.headers);
          /* istanbul ignore next */
          cov_1achjk6u4t().s[152]++;
          if (
          /* istanbul ignore next */
          (cov_1achjk6u4t().b[50][0]++, customHeaders) &&
          /* istanbul ignore next */
          (cov_1achjk6u4t().b[50][1]++, typeof customHeaders === "string")) {
            /* istanbul ignore next */
            cov_1achjk6u4t().b[49][0]++;
            cov_1achjk6u4t().s[153]++;
            headers = JSON.parse(customHeaders);
          } else
          /* istanbul ignore next */
          {
            cov_1achjk6u4t().b[49][1]++;
          }
          const response =
          /* istanbul ignore next */
          (cov_1achjk6u4t().s[154]++, await fetch(fullUrl, {
            method: "GET",
            headers: headers
          }));
          /* istanbul ignore next */
          cov_1achjk6u4t().s[155]++;
          if (!response.ok) {
            /* istanbul ignore next */
            cov_1achjk6u4t().b[51][0]++;
            cov_1achjk6u4t().s[156]++;
            data = {
              error: response.statusText
            };
          } else {
            /* istanbul ignore next */
            cov_1achjk6u4t().b[51][1]++;
            cov_1achjk6u4t().s[157]++;
            data = await response.json();
          }
        }
        /* istanbul ignore next */
        cov_1achjk6u4t().s[158]++;
        messages.push({
          tool_call_id: toolCall.id,
          role: "tool",
          name: functionName,
          content: JSON.stringify(data)
        });
      }
    } else
    /* istanbul ignore next */
    {
      cov_1achjk6u4t().b[38][1]++;
    }
    const secondResponse =
    /* istanbul ignore next */
    (cov_1achjk6u4t().s[159]++, await openai.chat.completions.create({
      model: chatSettings.model,
      messages,
      stream: true
    }));
    const stream =
    /* istanbul ignore next */
    (cov_1achjk6u4t().s[160]++, (0, ai_1.OpenAIStream)(secondResponse));
    /* istanbul ignore next */
    cov_1achjk6u4t().s[161]++;
    return new ai_1.StreamingTextResponse(stream);
  } catch (error) {
    /* istanbul ignore next */
    cov_1achjk6u4t().s[162]++;
    console.error(error);
    const errorMessage =
    /* istanbul ignore next */
    (cov_1achjk6u4t().s[163]++,
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[52][0]++, error.error?.message) ||
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[52][1]++, "An unexpected error occurred"));
    const errorCode =
    /* istanbul ignore next */
    (cov_1achjk6u4t().s[164]++,
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[53][0]++, error.status) ||
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[53][1]++, 500));
    /* istanbul ignore next */
    cov_1achjk6u4t().s[165]++;
    return new Response(JSON.stringify({
      message: errorMessage
    }), {
      status: errorCode
    });
  }
}
