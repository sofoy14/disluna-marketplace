  context += `   - Fuentes acadmicas: ${academicSources.length}\n`;
  /* istanbul ignore next */
  cov_1achjk6u4t().s[24]++;
  context += `   - Fuentes noticiosas: ${newsSources.length}\n`;
  /* istanbul ignore next */
  cov_1achjk6u4t().s[25]++;
  context += `   - Fuentes generales: ${generalSources.length}\n\n`;
  // Priorizar fuentes oficiales
  const prioritizedResults =
  /* istanbul ignore next */
  (cov_1achjk6u4t().s[26]++, [...officialSources, ...academicSources, ...newsSources, ...generalSources]);
  /* istanbul ignore next */
  cov_1achjk6u4t().s[27]++;
  prioritizedResults.forEach((result, index) => {
    /* istanbul ignore next */
    cov_1achjk6u4t().f[6]++;
    const sourceType =
    /* istanbul ignore next */
    (cov_1achjk6u4t().s[28]++, result.type === 'official' ?
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[5][0]++, '[OFICIAL]') :
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[5][1]++, result.type === 'academic' ?
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[6][0]++, '[ACADMICA]') :
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[6][1]++, result.type === 'news' ?
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[7][0]++, '[NOTICIAS]') :
    /* istanbul ignore next */
    (cov_1achjk6u4t().b[7][1]++, '[GENERAL]'))));
    /* istanbul ignore next */
    cov_1achjk6u4t().s[29]++;
