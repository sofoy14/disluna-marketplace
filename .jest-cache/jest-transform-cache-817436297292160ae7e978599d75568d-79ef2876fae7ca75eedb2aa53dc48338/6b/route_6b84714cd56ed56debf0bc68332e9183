    /* istanbul ignore next */
    cov_1achjk6u4t().s[30]++;
    context += ` URL: ${result.url}\n`;
    /* istanbul ignore next */
    cov_1achjk6u4t().s[31]++;
    context += ` Relevancia: ${result.relevance}/20\n`;
    /* istanbul ignore next */
    cov_1achjk6u4t().s[32]++;
    context += ` Contenido legal:\n${result.snippet}\n\n`;
    /* istanbul ignore next */
    cov_1achjk6u4t().s[33]++;
    context += `\n\n`;
  });
  /* istanbul ignore next */
  cov_1achjk6u4t().s[34]++;
  context += ` INSTRUCCIONES CRTICAS:\n`;
  /* istanbul ignore next */
  cov_1achjk6u4t().s[35]++;
  context += `1. USA NICAMENTE la informacin legal especfica encontrada arriba\n`;
  /* istanbul ignore next */
  cov_1achjk6u4t().s[36]++;
  context += `2. PRIORIZA fuentes oficiales (.gov.co) sobre otras fuentes\n`;
  /* istanbul ignore next */
  cov_1achjk6u4t().s[37]++;
  context += `3. NO uses informacin general si hay informacin especfica aqu\n`;
  /* istanbul ignore next */
  cov_1achjk6u4t().s[38]++;
  context += `4. Wikipedia est COMPLETAMENTE EXCLUIDA de esta bsqueda\n`;
  /* istanbul ignore next */
  cov_1achjk6u4t().s[39]++;
  context += `5. Responde con precisin jurdica colombiana\n\n`;
  /* istanbul ignore next */
  cov_1achjk6u4t().s[40]++;
  return context;
}
